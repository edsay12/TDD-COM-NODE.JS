{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "commonjs": true,
        "node": true,
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "import/no-unresolved":0,
        "import/extensions":0,
        "class-methods-use-this":0


        
    }
}
